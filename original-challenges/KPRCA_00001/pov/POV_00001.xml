<?xml version="1.0" standalone="no" ?>
<!DOCTYPE pov SYSTEM "/usr/share/cgc-docs/replay.dtd">
<pov>
    <cbid>service</cbid>
    <replay>
<!--
HELLO
OK A21D7B51
AUTH A21D7B51
OK
SET mode encode
OK
SET data aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa
OK
CALL /root64
-->
        <write>
            <data>HELLO\n</data>
        </write>

        <read echo="yes">
            <delim>\n</delim>
            <assign>
                <var>token</var>
                <pcre group="1">OK ([0-9A-F]*)\n</pcre>
            </assign>
        </read>

        <write>
            <data>AUTH </data>
            <var>token</var>
            <data>\n</data>
        </write>

        <read echo="yes">
            <delim>\n</delim>
            <match>
                <data>OK\n</data>
            </match>
        </read>

        <write>
            <data>SET mode encode\n</data>
        </write>

        <read echo="yes">
            <delim>\n</delim>
            <match>
                <data>OK\n</data>
            </match>
        </read>

        <write>
            <data>SET data aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa\n</data>
        </write>

        <read echo="yes">
            <delim>\n</delim>
            <match>
                <data>OK\n</data>
            </match>
        </read>

        <write>
            <data>CALL /root64\n</data>
        </write>

        <read echo="yes">
            <delim>\n</delim>
        </read>
    </replay>
</pov>
